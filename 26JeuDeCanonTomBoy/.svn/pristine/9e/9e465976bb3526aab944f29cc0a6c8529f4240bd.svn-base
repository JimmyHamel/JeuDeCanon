package interfaces;

import java.util.EventListener;
/**
 * Ecouteur personnalise detectant la creation d'un nouveau profil
 * @author Jimmy
 *
 */
public interface NewProfilListener extends EventListener{
	/**
	 * Ecouteur de la creation d'un nouveau profil
	 * @param isAProfilCreated = boolean disant si un profil a été cree ou pas
	 */
	public void  profilCree(boolean isAProfilCreated);

}
